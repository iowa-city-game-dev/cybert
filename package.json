{
  "name": "cybert",
  "version": "0.0.0",
  "description": "A Discord bot for the Iowa City Game Dev Meetup.",
  "repository": {
    "type": "git",
    "url": "https://github.com/iowa-city-game-dev/cybert.git"
  },
  "author": "Iowa City Game Dev Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iowa-city-game-dev/cybert/issues"
  },
  "homepage": "https://github.com/iowa-city-game-dev/cybert#readme",
  "scripts": {
    "clean": "shx rm -rf dist",
    "lint": "eslint src test",
    "test": "npm run clean && tsc --project tsconfig.test.json && jasmine --config=jasmine.json",
    "build": "npm run clean && tsc",
    "start:watch": "shx mkdir dist && shx echo > dist/index.js && concurrently -n tsc,node \"tsc --watch --preserveWatchOutput\" \"nodemon --watch dist dist/index.js\"",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "discord.js": "12.5.3",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/jasmine": "3.7.0",
    "@types/node": "14.14.41",
    "@types/ws": "7.4.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "concurrently": "6.0.2",
    "eslint": "7.25.0",
    "jasmine": "3.7.0",
    "nodemon": "2.0.7",
    "shx": "0.3.3",
    "typescript": "4.2.4"
  }
}
